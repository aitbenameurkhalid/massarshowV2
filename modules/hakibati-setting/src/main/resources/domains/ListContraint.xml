    <boolean name="afficherProf" title="afficher prof"/>
    <boolean name="afficherSalle" title="afficher Salle"/>
    <boolean name="afficherMatiere" title="afficher Matiere"/>
    <boolean name="afficherFillier" title="afficher Fillier"/>
    <boolean name="afficherMatin" title="afficher Matin"/>
    <boolean name="afficherApresMidi" title="afficher Aprés Midi"/>
    <boolean name="afficherDateExam" title="afficher Dtae Exam"/>
    <integer name="valeurParDefaut" title="valeur par défaut"/>
    <string name="commenter" title="commenter"/>
  </entity>
</domain-models>
