<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="hakibati-setting" package="com.hakibati.setting.db" />
  <entity name="ActivationUser" cacheable="true" >
    <string name="email" required="true" title="Email" unique="true" />
    <string name="codeActivationHash" required="true" title="code d'activation"/>
    <datetime name="datetime" title="DateTime"/>
    <integer name="numberTest" title="Number de test" default="0"/>
  </entity>
</domain-models>
