      <field name="ppr"/>
      <field name="matiere"/>
      <field name="name" title="nomCompletAr"/>
      <field name="nomCompletFr" title="nomCompletFr"/>
    </panel>
    <panel-related field="affectationEnseignant" colSpan="12"/>
  </form>

</object-views>
